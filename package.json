{
  "name": "js-accessor",
  "version": "1.2.0",
  "description": "Provides accessors like get, set, assertGet, clone, and toObject for a JS object",
  "author": {
    "email": "ywordk@gmail.com",
    "name": "softkave"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/softkave/js-accessor.git"
  },
  "bugs": {
    "url": "https://github.com/softkave/js-accessor/issues"
  },
  "homepage": "https://github.com/softkave/js-accessor",
  "type": "module",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src/{*.js,**/*.js}",
    "build/src/{*.js.map,**/*.js.map}",
    "build/src/{*.d.ts,**/*.d.ts}",
    "!build/src/**/__tests__/**/*"
  ],
  "license": "MIT",
  "keywords": [
    "accessor",
    "getter",
    "setter",
    "assertion",
    "clone"
  ],
  "scripts": {
    "test": "jest --config=jest.config.ts --detectOpenHandles",
    "lint": "gts lint",
    "clean": "gts clean && rm -f tsconfig.tsbuildinfo",
    "compile": "tsc",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "publish:npm": "npm run clean && tsc && npm publish",
    "show:packlist": "npm run clean && tsc && npm pack --dry-run",
    "publish:local": "npm run clean && tsc && yalc publish"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.199",
    "@types/node": "20.8.2",
    "gts": "^5.1.1",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "~5.2.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "softkave-js-utils": "^0.16.2",
    "type-fest": "^4.4.0"
  }
}
